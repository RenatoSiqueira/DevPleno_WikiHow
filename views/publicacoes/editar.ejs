<%- include('../header') %>

    <form action="/publicacoes/editar/<%= categoria %>/<%= publicacao.id %>" method="post">
    <div class="form-group">
        <label for="titulo">Título</label>
        <input type="text" class="form-control" id="titulo" name="titulo" placeholder="Título Publicação" value="<%= publicacao.titulo %>">
    </div>
    <div class="form-group">
        <label for="conteudo">Conteúdo</label>
        <textarea class="form-control" id="conteudo" name="conteudo" placeholder="Conteúdo"><%= publicacao.conteudo %></textarea>
    </div>
    <button type="submit" class="btn btn-primary block">Salvar</button>
    </form>

<%- include('../footer') %>